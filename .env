# .env - Configuración simple

# ✅ AQUÍ DEFINES QUÉ JSON USAR (el nombre sin .json)
# Opciones: default, biomed, coosalud, medicorp, o el nombre de tu archivo JSON
DEFAULT_TENANT=biomed

# Configuración básica
DEBUG=true
APP_NAME=Sistema de Gestión
APP_VERSION=1.0.0

# Servidor
HOST=0.0.0.0
PORT=8080

# APIs
AUTH_API_URL=http://localhost:9000
DATA_API_URL=http://localhost:8000

# Seguridad
SECRET_KEY=tu-clave-secreta-aqui

# Logging
LOG_LEVEL=INFO